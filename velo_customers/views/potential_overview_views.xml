    <record id="action_velo_hos_potential" model="ir.actions.act_window">
        <field name="name">Potentials - HOS</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[('customer_rank','>', 0),('tenant','>', 0),('industry.pop_industry_name','ilike', 'hos')]</field>
        <field name="context">{'res_partner_search_mode': 'realcustomer','default_is_company': True}</field>
        <field name="filter" eval="True"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Create a new customer in your address book
            </p><p>
            Odoo helps you easily track all activities related to a customer.
            </p>
        </field>
    </record>

